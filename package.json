{
  "name": "sn-typescript-util",
  "version": "1.4.0",
  "description": "A TypeScript utility for ServiceNow developers using VS Code",
  "bin": {
    "snts": "bin/snts.js"
  },
  "keywords": [
    "ServiceNow",
    "Service Portal",
    "TypeScript"
  ],
  "author": "Steven Gregory",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevengregory/sn-typescript-util.git"
  },
  "scripts": {
    "format": "prettier --write ./bin/*.js ./scripts/*.ts",
    "lint": "eslint .",
    "release": "bun ./scripts/utils/release.ts",
    "watch": "tsc --watch"
  },
  "type": "module",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "colorette": "^2.0.20",
    "commander": "^12.0.0",
    "execa": "^8.0.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "bun-types": "^1.1.3",
    "eslint": "^9.0.0",
    "prettier": "^3.2.5"
  }
}
